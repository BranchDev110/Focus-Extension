{
  "name": "Notify!",
  "description": "A Google Chrome extension!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "nativeMessaging"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "./assets/js/background.js",
    "type": "module"
  }
}